		 <script type="text/javascript" src="./scripts/js/jquery-ui-1.7.2.custom.min.js"></script>
		<script type="text/javascript">
		
			var assetCategory="固定资产";
			$(document).ready(function(){
				init();
				 $('#tabs').tabs(
					);
			var deptID;
			showDeptAndPeo("#dept32","#person32");	
			showFAssetKindList("#sel_fassetKind");
			changeNoAndName("#sel_fassetKind","#sel_fassetNo","#text_fassetName","#text_fassetModel","#text_fassetDept","#text_fassetOwner");	
			function init(){
				$("#header").showHeaderSystem();
				$("#footer").showFooter();
				$("#sidebar").showSideBar();
				$(this).navi();
			}
			
			function showFAssetKindList(kindSel){
				//获取固定资产类别列表
				$.post('/assetKindShowList',{'assetCategory':assetCategory},
					CB_kindList,
					'xml');
				function CB_kindList(xml){
					$(kindSel).append("<option value=\"\">"+"—类别—"+"</option>");
					$(xml).find("item").each(function(){
						$(kindSel).append("<option value=\""+$(this).find("assetKindID").text()+"\">"+$(this).find("assetKind").text()+"</option>");									
					});	
				}
			}
			
			function changeNoAndName(kindSel,noSel,nameText,modelText,deptText,perText)
			{
				$(kindSel).change(
					function ()
					{	
						
							var perext=noSel+" option";
							$(perext).remove();
							var kindval = $(kindSel).val();
							$(noSel).append("<option value=\"\">"+"—编号—"+"</option>");
							$(nameText).val("");
							$(modelText).val("");
							$(deptText).val("");
							$(perText).val("");
							if(kindval=="")
							{
								return;
							}
							$.post('/assetFixedListAlloted',
								{'page':null,'assetKindID':kindval},
								function(xml){
								$(xml).find("item").each(function(){
								$(noSel).append("<option value=\""+$(this).find("assetNo").text()+"\">"+$(this).find("assetNo").text()+"</option>");
								
								});
								$(noSel).change(
									function()
									{
										var assetNo=$(noSel).val();
										if(assetNo=="")
										{
											$(nameText).val("");
											$(deptText).val("");
											$(perText).val("");
											return;
										}
										$(xml).find("item").each(function(){
										if($(this).find("assetNo").text()==assetNo)
										{
											$(nameText).val($(this).find("assetName").text());
											$(modelText).val($(this).find("assetModel").text());
											$(perText).val($(this).find("assetOwner").text());
											$(deptText).val($(this).find("assetDept").text());
											deptID=$(this).find("assetDeptID").text();
										}
										
										});
									}
								);
								
								},
								'xml');
						
					}		
				);
			}
			
			function showDeptAndPeo(dept,person){
				//获取部门以及该部门的成员
				$.post('/systemDeptList',{},
					function(xml){
						$(dept).append("<option value=\"\">"+"—部门—"+"</option>");
						$(xml).find("item").each(function(){
							$(dept).append("<option value=\""+$(this).find("deptId").text()+"\">"+$(this).find("deptName").text()+"</option>");									
						});
																	
						$(person).append("<option value=\"\">"+"—人员—"+"</option>");
						
						},
					'xml');
																	
				 //人员随着部门改变而改变
			 	$(dept).change(function(){
			 		var perext=person+" option";
					$(perext).remove();
					var deptval = $(dept).val();
					if(deptval=="")
					{
						$(person).append("<option value=\"\">"+"—人员—"+"</option>");
						return;
					}
					$.post('/gotUserbyDept',
							{'dept':deptval},
							function(xml){
							$(person).append("<option value=\"\">"+"—人员—"+"</option>");
							$(xml).find("item").each(function(){
							$(person).append("<option value=\""+$(this).find("userName").text()+"\">"+$(this).find("userName").text()+"</option>");
							});
					},
				'xml');
			
				});
			}
			
			$("#comfirm").click
			(
				function()
				{
					var assetKindID=$("#sel_fassetKind").val();
					var assetNo		=$("#sel_fassetNo").val();
					var assetName	=$("#text_fassetName").val();
					var assetModel	=$("#text_fassetModel").val();
					var assetRecyTime	=$("#fassetRecyTime").val();
					var assetOwner=$("#text_fassetOwner").val();
					var assetOper=$("#person32").val();
					var assetOperDeptID=$("#dept32").val();
					var assetRecyReason=$("#recyclereason").val();
					var array=[assetKindID,assetNo,assetName,assetModel,assetRecyTime,assetOwner,assetOper,assetOperDeptID,deptID];
					for(i=0;i<9;i++){
						if(array[i]==""){
							alert("请填入完整信息！");return;
						}
					}
					$.post('/fassetRecycle',{'assetCategory':assetCategory,'assetKindID':assetKindID,'assetNo':assetNo,'assetName':assetName,'assetModel':assetModel,'assetOpeTime':assetRecyTime,'assetOwner':assetOwner,'assetDept':deptID,'assetOper':assetOper,'assetOperDeptID':assetOperDeptID,'recycleReason':assetRecyReason},CB_fassetRecy,'xml');
						function CB_fassetRecy(databack){
							var recode = $(databack).find("recode").text();
							if(recode != 0){
								alert($(databack).find("errormsg").text());
							}else {
									alert("回收成功！");
									location.href="fassetrecycle.html";
							}
						}
				}
				
			);
			
			}); 
		</script>
		 
