			};
			$(this).showData(url,page);
			function init(){
					$("#header").showHeaderSystem();
					$("#footer").showFooter();
					$("#sidebar").showSideBar();
					$(this).navi();
				}
				
				//注册按钮
				function register() {
	               //点击"删除"按钮，弹出是否删除的询问对话框
	               $(".delete").each(function(index){
						var $this = $(this);
						var id = $this.parents('.content').attr('id');
                        
						$this.click(function(){
							if(confirm("您确定删除吗？")){
								$.post('/overtimeDelete', {'applyId':id}, delCallback, 'xml');
		                	}
		                });
	               });
	            }
				
				//返回成功与否的判断与跳转
			    function delCallback(databack){
					var recode = $(databack).find("recode").text();
					if(recode != 0){
						alert($(databack).find("errormsg").text());
					}else {
						location.href="overtimeInfoList.html?page="+page;
					}
				}
				
				function register2(){
				   var currentuserId="";
				   var deptId="";
				   $.post('/currentUserDetail',
						     {},
						     function(xml){
				                currentuserId   = $(xml).find("userId").text();
				                deptId= $(xml).find("userDeptId").text();
			           //点击"删除"按钮，弹出是否删除的询问对话框
		               $(".approval2").each(function(index){
							var $this = $(this);
							var id = $this.parents('.content').attr('id');
							var row=$this.parent("td").parent("tr");
							var content=$("#bmJudgement",row).text();
							var userId=$("#userId",row).text();
						    var state1="等待部门主管审批";
						    var state2="等待分管领导审批";
						    var state3="等待人事部审批";
						    var state4="部门主管审批不通过";
						    var state5="分管领导审批不通过";
						    var state6="人事部审批通过";
						    var state7="人事部审批不通过";
						    var personInCharge=$("#personInCharge",row).text();
						    var secondManager=$("#secondManager",row).text();
						    $this.attr("disabled", true);
						      
						                 if(content==state1 && currentuserId==personInCharge){
						                        $this.attr("disabled", false);
						                        $this.click(function(){
							                          tmp="applyId="+id;
									                  location.href="/approvalOvertime.html?"+tmp;
			                                    });
						                  }
						                  
						                  if(content==state2 && secondManager==currentuserId){
						                        $this.attr("disabled", false);
						                        $this.click(function(){
							                          tmp="applyId="+id;
									                  location.href="/approvalOvertime.html?"+tmp;
			                                    });
						           
						                 }
						
						    if(content==state3){
						            $.post('/getPermission',
				                          {},
				                          function(xml) {
					                            permission = $(xml).find("result").text();
					                            acceptpermission = $(xml).find("permission").find("ticketApplyInfo").find("vacancy").text();
					                            if (acceptpermission == 0) {
							                        $this.attr("disabled", true);
						                        }else{
						                            $this.attr("disabled", false);
						                        }
						                   },
						            'xml');
						            
						//            $this.attr("disabled", false);
						            $this.click(function(){
							            tmp="applyId="+id;
									    location.href="/approvalOvertime.html?"+tmp;
			                       });
						    }
						    
						    if(userId==currentuserId && (content==state4 || content==state5 || content==state7)){
						           $this.attr("disabled", false);
						           $this.click(function(){
							            tmp="applyId="+id;
									    location.href="/editOvertime.html?"+tmp;
			                       });
						    }
						    
						     });
						     
						     },
						  'xml');
						    
		               
			     }
		});  
	</script>

