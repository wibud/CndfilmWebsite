<script type="text/javascript" src="./scripts/jquery/jquery-ui-1.8.16.custom.min.js"></script>
	<script type="text/javascript">
	    $(document).ready(function(){
		    init();
			$(".tabs").tabs({
					collapsible: true,
					selected: 1
			});	
			$("#ticket").click(function() {
				location.href = "ticketInfoList.html";
			});
			$("#overtime").click(function() {
				location.href = "overtimeInfoList.html";
			});			
			$("#vacancy").click(function() {
				location.href = "vacancyInfoList.html";
			});
			$("#outBound").click(function() {
				location.href = "outBoundApplyList.html";
			});
						
			//翻页
			var href = location.href;
			var page = href.split("page=")[1];
			if(page)
			   page = page.split("&")[0].split("#")[0];
			if(!page || page<1)
			   page=1;
			var pageCount = 0;
			var count = 0;
			var url = "/overtimeApplyList";
			
			var currentuserId="";
			$.post('/currentUserDetail',
					{},
				    function(xml){
				        currentuserId=$(xml).find("userId").text();
				    },
				    'xml');
			
			$.fn.showData = function(url,page2){
			   page = page2;
			   urlT = url+"?page="+page2;
			   $(this).handlerData(
			            urlT,
						function(xml){
							$("#content_r7 table tr:gt(0)").remove();
							var id = (page2-1)*20 + 1;
							count = $(xml).find("result").attr("count");
							pageCount = $(xml).find("result").attr("pageCount");
							var startWith = $(xml).find("result").attr("startWith");

							var recode = $(xml).find("recode").text();
							if(recode){
								alert($(xml).find("errormsg").text());
								}
							$(xml).find("item").each(function(i){
							     var applyId = $(this).find("applyId").text();
							     var userId = $(this).find("userId").text();
								 var userName = $(this).find("userName").text();
								 var userDept = $(this).find("deptName").text();
								 var applyTime = $(this).find("applyTime").text();
								 var applyReason = $(this).find("applyReason").text();
								 var applyStartTime = $(this).find("applyStartTime").text();
								 var applyEndTime = $(this).find("applyEndTime").text();
								 var totalHour = $(this).find("totalHour").text();
								 var bmJudgement = $(this).find("bmJudgement").text();
								 var personInCharge= $(this).find("personInCharge").text();
								 var secondManager= $(this).find("secondManager").text();
								 
								 var tmp="applyId="+applyId;
								 var bookingInfo1="<tr class='content' id='"+applyId+"'><td>"+(i+1)+"</td><td>"+userName+"</td><td>"+userDept+"</td><td>"+applyTime+"</td><td>"+applyStartTime+"</td><td>"+applyEndTime+"</td><td>"+totalHour+"</td><td id='bmJudgement'>"+bmJudgement+"</td>";
								 
								 var bookingInfo2="<td><input  type='image' src='./images/oa_detail.png'  title='查看详情' onclick=\"javascript: location.href=encodeURI(\'overtimeApplyDetail.html?"+tmp+"&page1="+page+"\')\"></input></td>";
								 
								 if(currentuserId==userId && bmJudgement=="等待部门主管审批"){
								      var bookingInfo4="<td><input  type='image' src='./images/oa_edit.gif'  title='修改' onclick=\"javascript: location.href=encodeURI(\'editOvertime.html?"+tmp+"&page1="+page+"\')\"></input></td><td id='delete'><input type='image' src='./images/oa_delete.png'  title='删除' class='delete'></input></td>";
								 }else if(currentuserId==userId && (bmJudgement=="部门主管审批不通过" || bmJudgement=="分管领导审批不通过" || bmJudgement=="人事部审批不通过")){
								      var bookingInfo4="<td><input  type='image' src='./images/oa_edit_g.gif'></input></td><td id='delete'><input type='image' src='./images/oa_delete.png'  title='删除' class='delete'></input></td>";
								 }else{
								      var bookingInfo4="<td><input  type='image' src='./images/oa_edit_g.gif'></input></td><td><input type='image' src='./images/oa_delete_g.png' title='删除'/></td>";
								 }
								 
								 if(bmJudgement=="部门主管审批不通过" || bmJudgement=="分管领导审批不通过" || bmJudgement=="人事部审批不通过"){
								      var bookingInfo3="<td id='approval2'><input type='button' value='修改' title='修改' class='approval2'></input></td>"+"<td id='personInCharge' style='display:none'>"+personInCharge+"</td><td id='secondManager' style='display:none'>"+secondManager+"</td><td id='userId' style='display:none'>"+userId+"</td></tr>";
								 }else{
								      var bookingInfo3="<td id='approval2'><input type='button' value='审批' title='审批' class='approval2'></input></td>"+"<td id='personInCharge' style='display:none'>"+personInCharge+"</td><td id='secondManager' style='display:none'>"+secondManager+"</td><td id='userId' style='display:none'>"+userId+"</td></tr>";
								 }     
								 $("#content_r7 table").append(bookingInfo1+bookingInfo2+bookingInfo4+bookingInfo3);	
								 id++;							 
							});
						//	$("#pageNav").showPageNav(page2, pageCount, count, url);
							 pageNav(page2,pageCount,count,url,"#pageNav");
							 register();
							 register2();
					}
