	window.parent.JqueryDialog.SubmitCompleted("修改阶段成功！", true, true , uurrll);
//	window.location.href="javascript:history.back()";
//	return true;//返回true模态窗口关闭；返回false模态窗口不关闭；
	};
	}
//	window.location.href = "projectInfoList.html";
	return false;
}
