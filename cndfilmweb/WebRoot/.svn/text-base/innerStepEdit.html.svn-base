	for(var i=0;i<dijitiao;i++){
		mubiao2+=mubiao[i]+"$";
	}
	mubiao2+=$('#mubiao').val()+"$";
	i+=1;
	for(;i<stepCount;i++){
		mubiao2+=mubiao[i]+"$";
	}


	
	if($('#projectStep').val() == ""){
