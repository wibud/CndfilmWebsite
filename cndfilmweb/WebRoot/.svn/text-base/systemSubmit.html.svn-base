	<head>
		<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
		<title>OA系统--导入用户信息</title>
		<link rel="stylesheet" href="./style/css/line.css" type="text/css" media="screen" charset="utf-8" />
		<script type="text/javascript" src="./scripts/js/page.js"></script>
		
		<script type="text/javascript">
			showIncludeFile();
			showIncludeAjaxingFile();
		</script>

		<script type="text/javascript" src="./scripts/js/ajaxfileupload.js"></script>
		
		
		<script type="text/javascript">
		$(document).ready(function(){
	    	init();	    	
		    function init(){
		    	$("#header").showHeaderSystem();
				$("#footer").showFooter();
				$("#sidebar").showSideBar();
				$(this).navi();
		    }
		    $("#id_imp").change(function() {
		    	var filepath = $('#id_imp').val();
		    	var reg = new RegExp(/\.pfx|\.p7b$/);
				if(!filepath.match(reg)){
						alert("请选择pfx或p7b格式的文件！");
						$('#id_imp').val(null);
						return;
					}
		    });

		    $('#upload').click(function(){
				var filepath = $('#id_imp').val();
				if(!filepath){
					alert("请选择文件");
					return false;
				}
				//if(filepath.substring(filepath.length - 4) != ".pem"){
				//	alert("上传文件要求为pem格式");
				//	return false;
				//}
				var reg = new RegExp(/\.pfx|\.p7b$/);
				if(!filepath.match(reg)){
						alert("请选择pfx或p7b格式的文件！");
						return;
					}
				
				$.ajaxFileUpload({
					url:			"uploadCert",
					type:			"POST",
					secureuri:		false,
					fileElementId:	"id_imp",
					dataType:		"xml",
					success:		function(xml, status){
										var recode = $(xml).find("recode").text();
					        			if(recode != 0){
					        				alert($(xml).find("errormsg").text());
					        			}else {
					        				alert("初始化成功！证书已导入！");
					        			}
									},
					error:			function(data, status, e){
										alert("初始化出错！");
									}
				});

				return false;
		    });

		});

	</script>


	</head>
