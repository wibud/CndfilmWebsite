										当前位置：日常管理 &gt;出差申请管理 &gt;&nbsp;
										<strong>查询出差申请</strong>
									</div>
								</td>
								<td>
									&nbsp;
								</td>
								<td align="right">
									<div class="biao2">
										<table border="0" cellspacing="0" cellpadding="0">
											<tr>
												<td>
													&nbsp;
												</td>
												<td width="10">
													&nbsp;
												</td>
												<td>
													&nbsp;
												</td>
												<td width="10">
													&nbsp;
												</td>
												<td>
													&nbsp;
												</td>
												<td width="10">
													&nbsp;
												</td>
												<td>
													&nbsp;
												</td>
											</tr>
										</table>
									</div>
								</td>
							</tr>
						</table>
					</div>
					<div align="center" id="content_xdao2">
						<table width="100%" border="0" cellspacing="0" cellpadding="0">
							<tr>
								<td width="14">
									<img src="images/bg_9.png" width="14" height="28" />
								</td>
								<td height="28" align="center" valign="middle" class="b_di">
									<strong>查询出差申请</strong>
								</td>
								<td width="14" align="right">
									<img src="images/bg_11.png" width="14" height="28" />
								</td>
							</tr>
						</table>
					</div>
					<div id="content_r2">
						<table class="sTable" border="0" cellpadding="0" cellspacing="0">
							<tr>
								<td>用户</td>
								<td class="width_15">
									<select name="deptId" id="deptId" class="dept" >
							        </select>
							        &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
							        <select name="userId" id="userId" class="person" >
							        </select>
							     </td>
							</tr>
					        <tr>
							      <td>申请时间</td>
						         <td class="width_15"><input id="createTime" style="width:150px" onclick="setday(this)" readonly="readonly"></input></td>
					        </tr>
					        <tr>
							      <td>出差时间</td>
						         <td class="width_15"><input id="leaveTime" style="width:150px" onclick="setday(this)" readonly="readonly"></input></td>
					        </tr>
					        <tr>
