package oa.model.table;
import javax.persistence.Entity;
import javax.persistence.Id;
import javax.persistence.Table;

import oa.dao.DatabaseAnno;

@DatabaseAnno(id="biztrip_id")
@Entity
@Table(name="tb_businesstrip")
public class BusinessTrip {
	@Id
	private String biztrip_id;		   		//标识
	private String user_id;		  			//用户名称
	private String dept_id;	 	  			//所在部门
	private String create_time;		    	//创建时间
	private String leave_time;				//出差时间
	private String back_time;	    		//返回时间
	private String destination;	    		//目的地
	private String biztrip_reason;	    	//出差原因
	private String follow_person;	    	//随行人员
	private String vehicle;	    			//交通工具
	private String manager_judgement;	    //主管意见
	private String bm_judgement;	    	//总经理意见
	private String remark;    				//备注
	private String bm_idea;
	private String manager_idea;
	
	public String getBiztrip_id() {
		return biztrip_id;
	}
	public void setBiztrip_id(String biztripId) {
		biztrip_id = biztripId;
	}
	public String getUser_id() {
		return user_id;
	}
	public void setUser_id(String userId) {
		user_id = userId;
	}
	public String getDept_id() {
		return dept_id;
	}
	public void setDept_id(String deptId) {
		dept_id = deptId;
	}
	public String getCreate_time() {
		return create_time;
	}
	public void setCreate_time(String createTime) {
		create_time = createTime;
	}
	public String getLeave_time() {
		return leave_time;
	}
	public void setLeave_time(String leaveTime) {
		leave_time = leaveTime;
	}
	public String getBack_time() {
		return back_time;
	}
	public void setBack_time(String backTime) {
		back_time = backTime;
	}
	public String getDestination() {
		return destination;
	}
	public void setDestination(String destination) {
		this.destination = destination;
	}
	public String getBiztrip_reason() {
		return biztrip_reason;
	}
	public void setBiztrip_reason(String biztripReason) {
		biztrip_reason = biztripReason;
	}
	public String getFollow_person() {
		return follow_person;
	}
	public void setFollow_person(String followPerson) {
		follow_person = followPerson;
	}
	public String getVehicle() {
		return vehicle;
	}
	public void setVehicle(String vehicle) {
		this.vehicle = vehicle;
	}
	public String getManager_judgement() {
		return manager_judgement;
	}
	public void setManager_judgement(String managerJudgement) {
		manager_judgement = managerJudgement;
	}
	public String getBm_judgement() {
		return bm_judgement;
	}
	public void setBm_judgement(String bmJudgement) {
		bm_judgement = bmJudgement;
	}
	public String getRemark() {
		return remark;
	}
	public void setRemark(String remark) {
		this.remark = remark;
	}
	public String getBm_idea() {
		return bm_idea;
	}
	public void setBm_idea(String bmIdea) {
		bm_idea = bmIdea;
	}
	public String getManager_idea() {
		return manager_idea;
	}
	public void setManager_idea(String managerIdea) {
		manager_idea = managerIdea;
	}

	
}



