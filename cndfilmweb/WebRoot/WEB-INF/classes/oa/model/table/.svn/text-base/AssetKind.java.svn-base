package oa.model.table;

import javax.persistence.Entity;
import javax.persistence.Id;
import javax.persistence.Table;

import oa.dao.DatabaseAnno;


@DatabaseAnno(id="assetKindID")
@Entity
@Table(name="tb_assetkind")
public class AssetKind {
	@Id
	private String assetKindID;		        //固定资产ID
	private String assetCategory;	       
	private String assetKind;
	public String getAssetKindID() {
		return assetKindID;
	}
	public void setAssetKindID(String assetKindID) {
		this.assetKindID = assetKindID;
	}
	public String getAssetCategory() {
		return assetCategory;
	}
	public void setAssetCategory(String assetCategory) {
		this.assetCategory = assetCategory;
	}
	public String getAssetKind() {
		return assetKind;
	}
	public void setAssetKind(String assetKind) {
		this.assetKind = assetKind;
	}	
	
	
	
	
	
}
