				var page = 1;
				var pageCount = 0;
				var count = 0;
				
				var url = "/overtimeSearch";
				
				var userId = tmp[0].split("=")[1];
				var deptId = tmp[1].split("=")[1];
				var applyTime = tmp[2].split("=")[1];
				var applyStartTime = tmp[3].split("=")[1];
				var applyEndTime = tmp[4].split("=")[1];
				var bmJudgement = tmp[5].split("=")[1];
				
				$.fn.showData = function(url,page2){
					page = page2;
					urlT = url;
					$(this).handlerData(
						urlT,
						//解析xml
						function(xml){
							$("#content_r7 table tr:gt(0)").remove();
