package oa.dao;

import oa.exception.LocalException;

@SuppressWarnings("serial")
public class DaoLocalException extends LocalException {

	public DaoLocalException(){
		super();
	}

	public DaoLocalException(String msg){
		super(msg);
	}

	public DaoLocalException(String msg, Throwable cause){
		super(msg, cause);
	}

	public DaoLocalException(Throwable cause){
		super(cause);
	}
}
