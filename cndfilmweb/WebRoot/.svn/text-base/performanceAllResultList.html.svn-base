						month = (assessPeriod.split(";")[1]).split(",")[0];
						return year + "年" + month + "月";
					} else {
						if(assessPeriod.split(",")[2]== "3")
							return year + "年一季度";
						if(assessPeriod.split(",")[2]== "6")
							return year + "年二季度";
						if(assessPeriod.split(",")[2]== "9")
							return year + "年三季度";
						if(assessPeriod.split(",")[2]== "12")
