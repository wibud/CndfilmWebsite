		pjStep2+=pjStep[i]+"$";
	}
	pjStep2+=$('#projectStep').val()+"$";
	i+=1;
	for(;i<stepCount;i++){
		pjStep2+=pjStep[i]+"$";
	}
	
	var miaoshu2='';
	for(var i=0;i<dijitiao;i++){
		miaoshu2+=miaoshu[i]+"$";
	}
	miaoshu2+=$('#miaoshu').val()+"$";
	i+=1;
	for(;i<stepCount;i++){
		miaoshu2+=miaoshu[i]+"$";
	}
	
	var deStart2='';
	for(var i=0;i<dijitiao;i++){
		deStart2+=deStart[i]+",";
	}
	deStart2+=$('#deStartTime').val()+",";
	i+=1;
	for(;i<stepCount;i++){
		deStart2+=deStart[i]+",";
	}

	var deEnd2='';
	for(var i=0;i<dijitiao;i++){
		deEnd2+=deEnd[i]+",";
	}
	deEnd2+=$('#deEndTime').val()+",";
	i+=1;
	for(;i<stepCount;i++){
		deEnd2+=deEnd[i]+",";
	}
	
