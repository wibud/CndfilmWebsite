							count = $(xml).find("result").attr("count");
							pageCount = $(xml).find("result").attr("pageCount");
							var startWith = $(xml).find("result").attr("startWith");
							
                      //      $("#pageNav").showPageNav(page2, pageCount, count, url);
							//判断recode，如果是0，则不弹出“errormsg”对话框
							var recode = $(xml).find("recode").text();
							if(recode){
								alert($(xml).find("errormsg").text());
							}
							$(xml).find("item").each(function(i){
	                             var applyId = $(this).find("applyId").text();
								 var userName = $(this).find("userName").text();
								 var userDept = $(this).find("deptName").text();
								 var applyTime = $(this).find("applyTime").text();
								 var applyReason = $(this).find("applyReason").text();
								 var applyStartTime = $(this).find("applyStartTime").text();
								 var applyEndTime = $(this).find("applyEndTime").text();
								 var totalHour = $(this).find("totalHour").text();
								 var bmJudgement = $(this).find("bmJudgement").text();
							     var personInCharge= $(this).find("personInCharge").text();
								 var secondManager= $(this).find("secondManager").text();
								 
   	                             var tmp="applyId="+applyId;
								 var bookingInfo1="<tr class='content' id='"+applyId+"'><td>"+(i+1)+"</td><td>"+userName+"</td><td>"+userDept+"</td><td>"+applyTime+"</td><td>"+applyStartTime+"</td><td>"+applyEndTime+"</td><td>"+totalHour+"</td><td id='bmJudgement'>"+bmJudgement+"</td>";
								 
								 var bookingInfo2="<td><input  type='image' src='./images/oa_detail.png'  title='查看' onclick=\"javascript: location.href=encodeURI(\'overtimeApplyDetail.html?"+tmp+"&page1="+page+"\')\"></input></td><td id='delete'><input type='image' src='./images/oa_delete.png'  title='删除' class='delete'></input></td>";
							      if(bmJudgement=="部门主管审批不通过" || bmJudgement=="分管领导审批不通过" || bmJudgement=="人事部审批不通过"){
								      var bookingInfo3="<td id='approval2'><input type='button' value='修改' title='修改' class='approval2'></input></td>"+"<td id='personInCharge' style='display:none'>"+personInCharge+"</td><td id='secondManager' style='display:none'>"+secondManager+"</td><td id='userId' style='display:none'>"+userId+"</td></tr>";
								 }else{
								      var bookingInfo3="<td id='approval2'><input type='button' value='审批' title='审批' class='approval2'></input></td>"+"<td id='personInCharge' style='display:none'>"+personInCharge+"</td><td id='secondManager' style='display:none'>"+secondManager+"</td><td id='userId' style='display:none'>"+userId+"</td></tr>";
								 }   
							
							//	 var bookingInfo3="<td id='approval'><input type='button' value='审批' title='审批' class='approval'></input></td></tr>";
								 $("#content_r7 table").append(bookingInfo1+bookingInfo2);	
								 id++;
							    
							  });
						//	  $("#pageNav").showPageNav(page2, pageCount, count, url);
							  pageNav(page2,pageCount,count,url,"#pageNav");
	                          //调用注册按钮的事件
						     register();
						     register1();
						},
						{"page":page2,"userId":userId,"deptId":deptId,"applyTime":applyTime,"applyStartTime":applyStartTime,"applyEndTime":applyEndTime,"bmJudgement":bmJudgement}
						);
				};
				
				
			   $(this).showData(url,page);
               
				function init(){
					$("#header").showHeaderSystem();
					$("#footer").showFooter();
					$("#sidebar").showSideBar();
					$(this).navi();
				}
				
                //注册按钮
				function register() {
	               //点击"删除"按钮，弹出是否删除的询问对话框
	               $(".delete").each(function(index){
						var $this = $(this);
						var id = $this.parents('.content').attr('id');
                        
						$this.click(function(){
							if(confirm("您确定删除吗？")){
								$.post('/overtimeDelete', {'applyId':id}, delCallback, 'xml');
		                	}
		                });
	               });
	            }
	            
	            function register1(){
				   var currentuserId="";
				   var deptId="";
				   $.post('/currentUserDetail',
						     {},
						     function(xml){
				                currentuserId   = $(xml).find("userId").text();
				                deptId= $(xml).find("userDeptId").text();
			           //点击"删除"按钮，弹出是否删除的询问对话框
		               $(".approval2").each(function(index){
							var $this = $(this);
							var id = $this.parents('.content').attr('id');
							var row=$this.parent("td").parent("tr");
							var content=$("#bmJudgement",row).text();
							var userId=$("#userId",row).text();
						    var state1="等待部门主管审批";
						    var state2="等待分管领导审批";
						    var state3="等待人事部审批";
						    var state4="部门主管审批不通过";
						    var state5="分管领导审批不通过";
						    var state6="人事部审批通过";
						    var state7="人事部审批不通过";
						    var personInCharge=$("#personInCharge",row).text();
						    var secondManager=$("#secondManager",row).text();
						    $this.attr("disabled", true);
						      
						                 if(content==state1 && currentuserId==personInCharge){
						                        $this.attr("disabled", false);
						                        $this.click(function(){
							                          tmp="applyId="+id;
								                      if(confirm("部门经理是否审批？")){
									                      location.href="/approvalOvertime.html?"+tmp;
			                	                      }
			                                    });
						                  }
						                  
						                  if(content==state2 && secondManager==currentuserId){
						                        $this.attr("disabled", false);
						                        $this.click(function(){
							                          tmp="applyId="+id;
								                      if(confirm("分管经理是否审批？")){
									                      location.href="/approvalOvertime.html?"+tmp;
			                	                      } 
			                                    });
						           
						                 }
						
						    if(content==state3 && deptId=="5"){
						            $this.attr("disabled", false);
						            $this.click(function(){
							            tmp="applyId="+id;
								        if(confirm("人事部是否审批？")){
									          location.href="/approvalOvertime.html?"+tmp;
			                	        }
			                       });
						    }
						    
						    if(userId==currentuserId && (content==state4 || content==state5 || content==state7)){
						           $this.attr("disabled", false);
						           $this.click(function(){
							            tmp="applyId="+id;
								        if(confirm("申请人是否修改？")){
									          location.href="/editOvertime.html?"+tmp;
			                	        }
			                       });
						    }
						    
						     });
						     
						     },
						  'xml');
						    
		               
			     }
	            
				//返回成功与否的判断与跳转
			    function delCallback(databack){
					var recode = $(databack).find("recode").text();
					if(recode != 0){
						alert($(databack).find("errormsg").text());
					}else {
						location.href="overtimeSearchList.html?userId=&deptId=&applyTime=&applyStartTime=&applyEndTime=&bmJudgement=";
					}
				}

			});

        </script>

