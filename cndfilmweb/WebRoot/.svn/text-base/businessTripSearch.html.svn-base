<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en">
	<head>
		<meta http-equiv="Content-Type" content="text/html; charset=utf-8"/>
		<title>OA系统--查询出差申请</title>
		<script type="text/javascript" src="./scripts/js/page.js"></script>
		<script type="text/javascript">
			showIncludeFile();
			showIncludeAjaxingFile();
			showIncludeThickBoxAjaxingFile();
		</script>

		<script type="text/javascript">
			//初始化页面
			$(document).ready(function(){

				init();
	                     
				$.post('/DeptList',
	                    {},
	                    function(xml){
	                    	$("#deptId").append("<option value=\"\">"+"—部门—"+"</option>");
							$(xml).find("item").each(function(){
									$("#deptId").append("<option value=\""+$(this).find("deptId").text()+"\">"+$(this).find("deptName").text()+"</option>");							
								});
							$.post('/getUserByDept',
				                    {'userDeptId':$('#deptId').val()},
				                    function(xml){
				                    	$("#userId").append("<option value=\"\">"+"—人员—"+"</option>");
										$(xml).find("item").each(function(){
												var id =$(this).find("userId").text();
												var name = $(this).find("userName").text();
												$("#userId").append("<option value=\""+id+"\">"+name+"</option>");
											});
				   	                 },
				                     'xml');							
	   	                 },
	                     'xml');
	                     
	                     
	                     
	         	$("#deptId").change(function(){
            		$("#userId option").remove();
            		var deptId=$('#deptId').val();
            		$.post('/getUserByDept',
            			{'userDeptId':deptId},
            			function(xml){
            				$("#userId").append("<option value=\"\">"+"—人员—"+"</option>");
            				$(xml).find("item").each(function(){
										$("#userId").append("<option value=\""+$(this).find("userId").text()+"\">"+$(this).find("userName").text()+"</option>");
	
									});
	
            			},
            		'xml');
            	});	
            	

				$('#comfirm').click(function(){
					var deptId      	= $('#deptId').val();
					var userId			= $('#userId').val();
					var createTime		= $('#createTime').val();
					var leaveTime		= $('#leaveTime').val();
					var destination		= $('#destination').val();
					
					var tmp="userId="+userId+"&deptId="+deptId+"&createTime="+createTime+"&leaveTime="+leaveTime+"&destination="+destination;
					tmp=encodeURI(tmp);
					location.href="/businessTripSearchList.html?"+tmp;
				});
				
				
				function init(){
					$("#header").showHeaderSystem();
					$("#footer").showFooter();
					$("#sidebar").showSideBar();
					$(this).navi();
				}
					
			});
		

		</script>
  </head>
